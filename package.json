    "execa": "^4.0.0",
    "find-up": "^5.0.0",
    "js-yaml": "^3.13.1",
    "keytar": "^7.0.0",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "p-limit": "^3.0.0",
    "p-map": "^4.0.0",
    "read": "^1.0.7",
