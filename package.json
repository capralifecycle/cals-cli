    "@types/js-yaml": "3.12.5",
    "@types/lodash": "4.14.160",
    "@types/node": "11.15.20",
    "@types/node-fetch": "2.5.7",
    "@types/read": "0.0.28",
    "@types/rimraf": "3.0.0",
    "@types/semver": "7.3.3",
    "@types/sprintf-js": "1.1.2",
    "@types/yargs": "15.0.5",
    "@typescript-eslint/eslint-plugin": "3.10.1",
