    "@types/jest": "26.0.19",
    "@types/js-yaml": "3.12.5",
    "@types/lodash": "4.14.165",
    "@types/node": "11.15.40",
    "@types/node-fetch": "2.5.7",
    "@types/read": "0.0.28",
    "@types/rimraf": "3.0.0",
    "@types/semver": "7.3.4",
    "@types/sprintf-js": "1.1.2",
    "@types/yargs": "15.0.11",
    "@typescript-eslint/eslint-plugin": "4.9.1",
    "@typescript-eslint/parser": "4.9.1",
    "dateformat": "3.0.3",
    "del": "6.0.0",
    "eslint": "7.15.0",
    "eslint-config-prettier": "7.0.0",
