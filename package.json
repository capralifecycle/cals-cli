{
  "name": "cals-cli",
  "version": "0.0.1",
  "description": "CLI for repeatable tasks in CALS",
  "scripts": {
    "build": "tsc",
    "test": "jest src",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "prettier": "prettier --config .prettierrc --write 'src/**/*.ts'",
    "prepare": "npm run build"
  },
  "bin": {
    "cals": "lib/cals-cli.js"
  },
  "dependencies": {
    "@octokit/rest": "16.25.0",
    "cachedir": "2.2.0",
    "chalk": "2.4.2",
    "js-yaml": "3.13.1",
    "keytar": "4.6.0",
    "node-fetch": "2.3.0",
    "read": "1.0.7",
    "sprintf-js": "1.1.2",
    "yargs": "13.2.2"
  },
  "devDependencies": {
    "@types/jest": "24.0.11",
    "@types/js-yaml": "3.12.1",
    "@types/node": "11.13.7",
    "@types/node-fetch": "2.3.2",
    "@types/read": "0.0.28",
    "@types/sprintf-js": "1.1.2",
    "@types/yargs": "13.0.0",
    "jest": "24.7.1",
    "prettier": "1.17.0",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.4.5"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/capralifecycle/cals-cli.git"
  },
  "types": "index.d.ts"
}
