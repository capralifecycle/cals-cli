    "@types/dateformat": "5.0.2",
    "ajv": "^8.11.0",
    "cachedir": "^2.4.0",
    "chalk": "4.1.2",
    "execa": "^5.0.0",
    "find-up": "^5.0.0",
    "js-yaml": "^4.1.0",
    "keytar": "^7.9.0",
    "node-fetch": "^3.0.0",
    "p-limit": "^3.0.0",
    "p-map": "^4.0.0",
    "process": "0.11.10",
    "read": "^1.0.7",
