    "husky": "4.2.5",
    "jest": "26.4.2",
    "jest-sonar-reporter": "2.0.0",
    "prettier": "2.1.1",
    "rollup": "2.26.5",
    "rollup-plugin-typescript2": "0.27.2",
    "semantic-release": "17.1.1",
    "tempy": "0.6.0",
    "ts-jest": "26.2.0",
    "typescript": "4.0.2",
    "typescript-json-schema": "0.43.0"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
