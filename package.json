    "eslint-plugin-prettier": "3.3.0",
    "husky": "4.3.5",
    "jest": "26.6.3",
    "jest-sonar-reporter": "2.0.0",
    "prettier": "2.1.2",
    "rollup": "2.34.2",
    "rollup-plugin-typescript2": "0.29.0",
    "semantic-release": "17.3.0",
    "tempy": "1.0.0",
    "ts-jest": "26.4.4",
    "typescript": "4.1.3",
    "typescript-json-schema": "0.45.0"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
