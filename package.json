  "devDependencies": {
    "@types/jest": "24.0.15",
    "@types/js-yaml": "3.12.1",
    "@types/lodash": "4.14.134",
    "@types/node": "11.13.14",
    "@types/node-fetch": "2.3.7",
    "@types/read": "0.0.28",
    "@types/sprintf-js": "1.1.2",
    "@types/yargs": "13.0.0",
    "@typescript-eslint/eslint-plugin": "1.10.2",
