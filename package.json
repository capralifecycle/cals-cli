    "prepack": "./scripts/build-and-verify.sh",
    "prettier": "prettier --config .prettierrc --write .",
    "semantic-release": "semantic-release",
    "watch": "rollup -c -w"
  },
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "types": "lib/index.d.ts",
  "bin": {
    "cals": "lib/cals-cli.mjs"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.0",
    "@types/dateformat": "5.0.3",
    "ajv": "^8.11.0",
    "cachedir": "^2.4.0",
    "chalk": "5.4.1",
    "execa": "^9.0.0",
    "find-up": "^7.0.0",
    "js-yaml": "^4.1.0",
    "keytar": "^7.9.0",
    "node-fetch": "^3.0.0",
    "p-limit": "^6.0.0",
    "p-map": "^7.0.0",
    "process": "0.11.10",
    "read": "^4.0.0",
    "semver": "^7.6.2",
    "sprintf-js": "^1.1.2",
    "yargs": "18.0.0"
  },
  "overrides": {
