    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.2.1",
    "husky": "9.1.7",
    "prettier": "3.4.2",
    "rollup": "2.79.2",
    "rollup-plugin-typescript2": "0.36.0",
    "semantic-release": "24.2.0",
    "tempy": "1.0.1",
    "tsx": "4.19.2",
    "typescript": "5.7.2",
